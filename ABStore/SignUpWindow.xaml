<Window x:Class="ABStore.SignUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ABStore"
        mc:Ignorable="d"
        Title="SignUpWindow" Height="566" Width="800" Closed="Window_Closed">
    <Window.Background>
        <ImageBrush ImageSource="D:\2 курс\Proga ABStore\Типы.jpg" Stretch="UniformToFill" TileMode="None" />
    </Window.Background>
    <Grid Margin="0,0,0,-31">
        <TextBox x:Name="LoginBox" HorizontalAlignment="Left" Height="20" Margin="208,505,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="172" Foreground="#FFF7F3F3" BorderBrush="#FFFFFEFE" Background="{x:Null}"/>
        <PasswordBox x:Name="_PasswordBox" HorizontalAlignment="Left" Height="20" Margin="420,505,0,0" VerticalAlignment="Top" Width="172" Background="{x:Null}" BorderBrush="#FFFFFEFE" Foreground="White"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="17" Margin="5,508,0,0" VerticalAlignment="Top" Width="97" Background="{x:Null}" Foreground="{x:Null}" BorderBrush="{x:Null}" Click="Button_Click" RenderTransformOrigin="0.5,0.5"/>

    </Grid>
</Window>
